{
  "plugins": [
    "unused-imports"
  ],
  "extends": [
    "@eyecuelab/react"
  ],
  "rules": {
    "max-len": "off",
    "max-lines": "off",
    // "unused-imports/no-unused-imports-ts": "error",
    "unused-imports/no-unused-vars-ts": "warn",
    "@typescript-eslint/ban-ts-comment": "off",
    "no-console": "warn",
    "semi": "warn",
    // "react/no-unknown-property": ["error", { "ignore": ["css", "intensity", "position"] }]
    "react/no-unknown-property": [
      "off"
    ]
  }
}